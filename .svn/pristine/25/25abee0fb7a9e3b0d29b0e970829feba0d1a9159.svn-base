package com.hjd.action.bean;

import java.math.BigDecimal;

import com.hjd.base.BeanAbs;
public class BusDepreciationHistBeanTemp extends BeanAbs{

        private Long equipmentId;//资源设备的ID
		
		private Long id;//折旧费登记的记录ID
		
		private BigDecimal depreciation;		//折旧费

		public Long getEquipmentId() {
			return equipmentId;
		}

		public void setEquipmentId(Long equipmentId) {
			this.equipmentId = equipmentId;
		}

		public Long getId() {
			return id;
		}

		public void setId(Long id) {
			this.id = id;
		}

		public BigDecimal getDepreciation() {
			return depreciation;
		}

		public void setDepreciation(BigDecimal depreciation) {
			this.depreciation = depreciation;
		}
		
		
	}
