package com.hjd.action.bean;

import com.hjd.base.IFPageRequest;

public class ParContactInfoSearchBean extends IFPageRequest {
	/**
	 * 企业标识/部门标识
	 * 初始查询时，送当前登录人员的企业标识
	 * 后续查询时，送选中的企业标识/部门标识
	 */
	private Long currOrgId;

	private Integer defConFlag;//默认联系人标识，只能有一个默认的联系人标识
	
	public Long getCurrOrgId() {
		return currOrgId;
	}
	public void setCurrOrgId(Long currOrgId) {
		this.currOrgId = currOrgId;
	}
	public Integer getDefConFlag() {
		return defConFlag;
	}
	public void setDefConFlag(Integer defConFlag) {
		this.defConFlag = defConFlag;
	}

	}
